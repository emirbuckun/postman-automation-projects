		{
			"name": "Acutal Request",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"const jsonData = pm.response.json();",
							"const cypressObject = jsonData.courses.webAutomation.find(x => x.courseTitle === \"Cypress\")",
							"const exceptedCourses = [\"Selenium Webdriver Java\", \"Cypress\", \"Protractor\"]",
							"",
							"pm.test(\"Course verification\", () => {",
							"    pm.expect(cypressObject).to.not.be.an(\"object\", \"Cypress not found\")",
							"});",
							"",
							"pm.test(\"Properties verification\", () => {",
							"    pm.expect(cypressObject).to.have.all.keys(\"courseTitle\", \"price\")",
							"});",
							"",
							"pm.test(\"Validate Sum of API Courses\", () => {",
							"    let sum = 0",
							"    for (let i = 0; i < jsonData.courses.api.length; i++) {",
							"        sum = sum + parseInt(jsonData.courses.api[i].price)",
							"    }",
							"    pm.expect(sum).to.equal(90)",
							"})",
							"",
							"pm.test(\"Validate Web Course Titles\", () => {",
							"    let actualCourses = []",
							"    jsonData.courses.webAutomation.map(x => actualCourses.push(x.courseTitle))",
							"    pm.expect(exceptedCourses).to.have.members(actualCourses)",
							"})"
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				}
			],
