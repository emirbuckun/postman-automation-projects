							"",
							"pm.test(\"Order details fetched successfully\", () => {",
							"    pm.expect(jsonData.message).to.eql(\"Orders fetched for customer Successfully\");",
							"});",
							"",
							"pm.test(\"Order ID matches\", () => {",
							"    pm.expect(jsonData.data._id).to.eql(pm.collectionVariables.get(\"orderId\"));",
							"});",
							"",
							"pm.test(\"Product ID matches\", () => {",
							"    pm.expect(jsonData.data.productOrderedId).to.eql(pm.collectionVariables.get(\"productId\"));",
							"});"
						],
