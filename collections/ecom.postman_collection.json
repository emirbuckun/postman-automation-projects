	"item": [
		{
			"name": "Login",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"const jsonData = pm.response.json();",
							"",
							"pm.test(\"Status code is 200\", () => {",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"pm.test(\"Response returns token, userId and success message\", () => {",
							"    pm.expect(jsonData).to.have.property(\"token\");",
							"    pm.expect(jsonData).to.have.property(\"userId\");",
							"    pm.expect(jsonData).to.have.property(\"message\");",
							"    pm.expect(jsonData.message).to.eql(\"Login Successfully\");",
							"});",
							"",
							"pm.test(\"Token is not empty\", () => {",
							"    pm.expect(jsonData.token).to.not.be.empty;",
							"});",
							"",
							"pm.test(\"User ID is not empty\", () => {",
							"    pm.expect(jsonData.userId).to.not.be.empty;",
							"});",
							"",
							"pm.collectionVariables.set(\"userId\", jsonData.userId);",
							"pm.collectionVariables.set(\"token\", jsonData.token);"
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\n    \"userEmail\": \"emirbuckun@hotmail.com\",\n    \"userPassword\": \"2901Emir.2901\"\n}",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
				"url": {
					"raw": "{{base_url}}/auth/login",
					"host": [
						"{{base_url}}"
					],
					"path": [
						"auth",
						"login"
					]
				}
			},
			"response": []
		},
