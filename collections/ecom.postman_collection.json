		{
			"name": "Create Order",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"const jsonData = pm.response.json();",
							"",
							"pm.test(\"Check if the order was placed successfully\", () => {",
							"    pm.expect(jsonData.message).to.eql(\"Order Placed Successfully\");",
							"});",
							"",
							"pm.test(\"Check if the order ID is present\", () => {",
							"    pm.expect(jsonData.orders).to.have.lengthOf(1);",
							"});",
							"",
							"pm.test(\"Check if the product order ID is present\", () => {",
							"    pm.expect(jsonData.productOrderId).to.have.lengthOf(1);",
							"});",
							"",
							"pm.collectionVariables.set(\"orderId\", jsonData.orders[0]);"
						],
